{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: Build TopMain",
            "command": "/usr/bin/g++",
            "args": [
                "*.cc",
                "-I/opt/umlrts/include/",
                "-L/opt/umlrts/lib/linux.x86-gcc-4.6.3",
                "-o",
                "TopMain",
                "-lrts",
                "-lpthread",
                "-lrt",
                "-lzmq",
                "-lzmqpp"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src-gen/cpp/src"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}